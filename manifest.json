{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "manifest_version": 2,
  "name": "Swagger UI Console",
  "description": "Swagger UI Chrome App",
  "version": "3.2.0",
  "author": "koaben",
  "homepage_url": "https://github.com/koaben",
  "icons": {
    "16": "swagger-ui/dist/favicon-16x16.png",
    "64": "swagger-ui/dist/favicon-32x32.png",
    "128": "swagger-ui/dist/favicon-32x32.png"
  },
  "permissions": [
    "https://*/",
    "http://*/",
    "file://*/*",
    "tabs",
    "clipboardWrite",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking"
  ],
  "background": {
    "scripts": [
      "04488849.js",
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": {
      "19": "swagger-ui/dist/favicon-16x16.png",
      "38": "swagger-ui/dist/favicon-32x32.png"
    },
    "default_title": "Swagger UI Console"
  }
}
